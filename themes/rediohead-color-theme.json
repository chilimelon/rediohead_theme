{
  "name": "rediohead theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": { "foreground": "#d19a66" },
    "variable.constant": { "foreground": "#e5c07b" },
    "variable.defaultLibrary": { "foreground": "#e5c07b" }
  },
  "colors": {
    "foreground": "#d9bcbc",
    "focusBorder": "#c73a3c",
    "selection.background": "#bb444430",

    "activityBar.background": "#2a1c1d",
    "activityBar.foreground": "#c73a3c",
    "activityBar.inactiveForeground": "#8a5f61",
    "activityBarBadge.background": "#8f2527",
    "activityBarBadge.foreground": "#f4dede",

    "sideBar.background": "#1f1819",
    "sideBar.foreground": "#bfa4a6",
    "sideBarSectionHeader.background": "#8f2527",
    "sideBarSectionHeader.foreground": "#f4dede",

    "editor.background": "#1c1414",
    "editor.foreground": "#d9bcbc",
    "editorLineNumber.foreground": "#a88e90",
    "editorLineNumber.activeForeground": "#f4dede",
    "editorCursor.foreground": "#c73a3c",
    "editor.selectionBackground": "#bb444440",
    "editor.inactiveSelectionBackground": "#a88e9026",
    "editor.wordHighlightStrongBackground": "#a88e9026",
    "editor.wordHighlightBackground": "#a88e9018",
    "editor.lineHighlightBackground": "#a88e9018",
    "editor.lineHighlightBorder": "#2a1c1d",
    "editorIndentGuide.background": "#5f3a3b",
    "editorIndentGuide.activeBackground": "#c73a3c",
    "editorBracketMatch.background": "#c73a3c40",
    "editorBracketMatch.border": "#c73a3c",

    "editorGutter.background": "#1c1414",
    "editorGutter.modifiedBackground": "#0c7d9d",
    "editorGutter.addedBackground": "#587c0c",
    "editorGutter.deletedBackground": "#94151b",

    "editorError.foreground": "#ff6d6d",
    "editorWarning.foreground": "#ffd668",
    "editorInfo.foreground": "#7ee2ff",

    "statusBar.background": "#2a1c1d",
    "statusBar.foreground": "#bfa4a6",
    "statusBarItem.remoteBackground": "#c73a3c",
    "statusBarItem.remoteForeground": "#f4dede",

    "titleBar.activeBackground": "#2a1c1d",
    "titleBar.activeForeground": "#d9bcbc",
    "titleBar.inactiveBackground": "#2a1c1d",
    "titleBar.inactiveForeground": "#bfa4a6",

    "tab.activeBackground": "#1c1414",
    "tab.activeForeground": "#f4dede",
    "tab.inactiveBackground": "#2a1c1d",
    "tab.inactiveForeground": "#bfa4a6",
    "tab.hoverBorder": "#c73a3c",

    "badge.background": "#8f2527",
    "badge.foreground": "#f4dede",

    "list.inactiveSelectionBackground": "#8f252766",
    "list.inactiveSelectionForeground": "#ffffff",
    "list.hoverBackground": "#8f252730",
    "list.activeSelectionBackground": "#4b0e0f",
    "list.activeSelectionForeground": "#f4dede",

    "terminal.foreground": "#d9bcbc",
    "terminalCursor.foreground": "#f4dede",
    "terminal.ansiRed": "#ff5a5a",
    "terminal.ansiGreen": "#67ff8a",
    "terminal.ansiBlue": "#74bcff",
    "terminal.ansiYellow": "#e1c542",
    "terminal.ansiMagenta": "#e557f9",
    "terminal.ansiCyan": "#39cde2",
    "terminal.ansiWhite": "#d4d4d4",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightRed": "#ff6d6d",
    "terminal.ansiBrightGreen": "#9affb2",
    "terminal.ansiBrightBlue": "#9cd0ff",
    "terminal.ansiBrightYellow": "#ffe05b",
    "terminal.ansiBrightMagenta": "#ec84ff",
    "terminal.ansiBrightCyan": "#74f2ff",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#7a6c6d" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff6d6d" }
    },
    {
      "name": "Keywords, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"],
      "settings": { "foreground": "#c678dd" }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation.separator.key-value",
        "punctuation.separator.comma",
        "punctuation.section.embedded"
      ],
      "settings": { "foreground": "#56b6c2" }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function",
        "meta.function-call",
        "meta.function-call.generic"
      ],
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable.name"],
      "settings": { "foreground": "#d9bcbc" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#bfa4a6" }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#98c379" }
    },
    {
      "name": "Numbers & Constants",
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.escape"],
      "settings": { "foreground": "#d19a66" }
    },
    {
      "name": "Types & Classes",
      "scope": ["support.type", "entity.name.type", "entity.name.class", "support.class"],
      "settings": { "foreground": "#e5c07b" }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#c73a3c" }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#d19a66" }
    },

    { "name": "JSON Keys", "scope": ["support.type.property-name.json"], "settings": { "foreground": "#c73a3c" } },

    { "name": "Markdown Headings", "scope": ["markup.heading", "markup.heading punctuation.definition.heading"], "settings": { "foreground": "#e5c07b" } },

    { "name": "Go package name", "scope": ["entity.name.package.go"], "settings": { "foreground": "#e5c07b" } },
    { "name": "Go builtin types", "scope": ["support.type.builtin.go"], "settings": { "foreground": "#e5c07b" } },
    { "name": "Go operators", "scope": ["keyword.operator.assignment.go", "keyword.operator.arithmetic.go", "keyword.operator.address.go"], "settings": { "foreground": "#56b6c2" } },

    { "name": "Python decorators", "scope": ["meta.function.decorator.python", "support.token.decorator.python"], "settings": { "foreground": "#61afef" } },
    { "name": "Python magic names", "scope": ["support.variable.magic.python"], "settings": { "foreground": "#c73a3c" } },
    { "name": "Python punctuation", "scope": ["punctuation.separator.period.python", "punctuation.separator.element.python", "punctuation.parenthesis.begin.python", "punctuation.parenthesis.end.python"], "settings": { "foreground": "#a88e90" } },
    { "name": "Python function calls", "scope": ["meta.function-call.generic.python"], "settings": { "foreground": "#61afef" } },
    { "name": "Python placeholders", "scope": ["constant.character.format.placeholder.other.python"], "settings": { "foreground": "#d19a66" } },

    { "name": "TypeScript primitives & builtins", "scope": ["support.type.primitive.ts", "support.type.builtin.ts", "support.type.primitive.tsx", "support.type.builtin.tsx"], "settings": { "foreground": "#e5c07b" } },
    { "name": "TypeScript types & interfaces", "scope": ["entity.name.type.ts", "entity.name.interface.ts"], "settings": { "foreground": "#e5c07b" } },
    { "name": "JS/TS imports", "scope": ["keyword.operator.expression.import"], "settings": { "foreground": "#61afef" } },
    { "name": "JS/TS properties", "scope": ["variable.other.property", "support.variable.property"], "settings": { "foreground": "#d9bcbc" } }
  ]
}

